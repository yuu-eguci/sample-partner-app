{
  "family": "sample-app",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "sample-app",
      "image": "foo",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        },
        {
          "hostPort": 443,
          "containerPort": 443,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "SHOPIFY_API_KEY",
          "valueFrom": "/sample-partner-app-env/SHOPIFY_API_KEY"
        },
        {
          "name": "SHOPIFY_API_SECRET",
          "valueFrom": "/sample-partner-app-env/SHOPIFY_API_SECRET"
        },
        {
          "name": "SCOPES",
          "valueFrom": "/sample-partner-app-env/SCOPES"
        },
        {
          "name": "HOST",
          "valueFrom": "/sample-partner-app-env/HOST"
        },
        {
          "name": "PORT",
          "valueFrom": "/sample-partner-app-env/PORT"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/task-definition-sample-app",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}
